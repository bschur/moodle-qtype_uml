{
  "name": "uml-editor",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve uml-editor",
    "build": "npm run build:clean && ng build uml-editor",
    "build:elements": "npm run build:clean && ng build uml-editor-elements",
    "build:elements:moodle": "npm run build:elements && cp -r ./dist/uml-editor-elements/browser/* ../dist",
    "build:clean": "rm -rf dist/"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^17.0.5",
    "@angular/compiler": "^17.0.5",
    "@angular/core": "^17.0.5",
    "@angular/elements": "^17.0.5",
    "@angular/platform-browser": "^17.0.5",
    "@angular/platform-browser-dynamic": "^17.0.5",
    "jointjs": "^3.7.7",
    "request": "^2.88.2",
    "rxjs": "~7.8.1",
    "tslib": "^2.6.2",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.5",
    "@angular/cli": "^17.0.5",
    "@angular/compiler-cli": "^17.0.5",
    "@types/backbone": "^1.4.18",
    "@types/jointjs": "^2.0.0",
    "@types/jquery": "^3.5.29",
    "typescript": "~5.2.2"
  }
}
