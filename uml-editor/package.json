{
  "name": "uml-editor",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "npm run build:clean && ng run uml-editor:build-app",
    "build:elements": "npm run build:clean && ng build",
    "build:elements:moodle": "npm run build:elements && cp -r ./dist/uml-editor-elements/* ../dist",
    "build:clean": "rm -rf dist/",
    "lint": "eslint src/**/*",
    "test": "ng test",
    "prettier": "prettier --write ."
  },
  "dependencies": {
    "@angular/animations": "^17.2.4",
    "@angular/cdk": "^17.2.2",
    "@angular/common": "^17.2.4",
    "@angular/compiler": "^17.2.4",
    "@angular/core": "^17.2.4",
    "@angular/elements": "^17.2.4",
    "@angular/forms": "^17.2.4",
    "@angular/material": "^17.2.2",
    "@angular/platform-browser": "^17.2.4",
    "@angular/platform-browser-dynamic": "^17.2.4",
    "jointjs": "^3.7.7",
    "rxjs": "~7.8.1",
    "tslib": "^2.6.2",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.2.3",
    "@angular-eslint/builder": "17.2.1",
    "@angular-eslint/eslint-plugin": "17.2.1",
    "@angular-eslint/eslint-plugin-template": "17.2.1",
    "@angular-eslint/schematics": "17.2.1",
    "@angular-eslint/template-parser": "17.2.1",
    "@angular/cli": "^17.2.3",
    "@angular/compiler-cli": "^17.2.4",
    "@types/backbone": "^1.4.18",
    "@types/jquery": "^3.5.29",
    "@typescript-eslint/eslint-plugin": "6.13.1",
    "@typescript-eslint/parser": "6.13.1",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "@types/jest": "^29.5.12",
    "ngx-build-plus": "^17.0.0",
    "prettier": "^3.1.1",
    "prettier-plugin-organize-imports": "^3.2.4",
    "typescript": "~5.2.2"
  }
}
